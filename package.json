{
  "name": "webpack-progress",
  "displayName": "Webpack Progress",
  "description": "Display Webpack build progress in VS Code Status Bar",
  "repository": {
    "type": "git",
    "url": "https://github.com/Combeenation/vscode-webpack-progress"
  },
  "version": "0.0.8",
  "publisher": "Combeenation",
  "engines": {
    "vscode": "^1.5.0"
  },
  "icon": "images/icon.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": []
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "tslint": "^5.11.0",
    "typescript": "^3.0.3",
    "vscode": "^1.1.21",
    "mocha": "^5.2.0",
    "@types/node": "^10.9.4",
    "@types/mocha": "^5.2.5"
  }
}
