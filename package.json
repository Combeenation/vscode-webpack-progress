{
  "name": "webpack-progress",
  "displayName": "Webpack Progress",
  "description": "Display Webpack build progress in VS Code Status Bar",
  "repository": {
    "type": "git",
    "url": "https://github.com/Combeenation/vscode-webpack-progress"
  },
  "version": "0.0.8",
  "publisher": "Combeenation",
  "engines": {
    "vscode": "^1.5.0"
  },
  "icon": "images/icon.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": []
  },
  "scripts": {
    "publish": "vsce publish",
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "tslint": "^5.11.0",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  }
}
